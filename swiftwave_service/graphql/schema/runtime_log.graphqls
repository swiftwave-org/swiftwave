type RuntimeLog {
    content: String!
    createdAt: Time!
}

enum RuntimeLogTimeframe {
    live
    last_1_hour
    last_3_hours
    last_6_hours
    last_12_hours
    last_24_hours
    lifetime
}

extend type Subscription {
    fetchRuntimeLog(applicationId: String!, timeframe: RuntimeLogTimeframe!): RuntimeLog!
}